{
  "manifest_version": 3,
  "name": "SnackStats",
  "description": "Give stats about the amount of money you have spent on food delivery apps.",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "scripting",
    "tabs"  
    
  ],
  "host_permissions": [
    "*://*.ubereats.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["*://*.ubereats.com/*"],
      "js": ["scripts/content.js"]
    }
  ],
  "action": {
    "default_popup": "popup/popup.html"
  }
}
